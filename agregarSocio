void agregarSocio(string ci,string nombre){
    for(int i = 0; i < cantSocios; i++){
        if(socios[i]->ci == ci) throw invalid_argument("El socio que intenta agregar ya es miembro");
    }
    
    if(cantSocios >= MAX_SOCIOS) throw invalid_argument("Se ha superado el limite de socios permitido");

    Socio* s = new Socio();
    s->ci = ci;
    s->nombre = nombre;
    socios[cantSocios] = s;
    cantSocios++;
}
